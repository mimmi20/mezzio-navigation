{
  "name": "mimmi20/mezzio-navigation",
  "type": "library",
  "description": "Provides a Navigation for Mezzio.",
  "keywords": [
    "laminas",
    "mezzio"
  ],
  "homepage": "https://github.com/mimmi20/mezzio-navigation",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-stdlib": "~3.2.1 || ~3.3.0",
    "psr/container": "^1.0.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "~2.8.0",
    "infection/infection": "~0.13.6 || ~0.17.6",
    "laminas/laminas-config": "~3.3.0 || ~3.4.0",
    "laminas/laminas-servicemanager": "~3.4.1",
    "mezzio/mezzio-authorization": "^1.0.1",
    "mezzio/mezzio-helpers": "^5.3.0",
    "mezzio/mezzio-router": "^3.1.3",
    "mimmi20/coding-standard": "~1.0.5",
    "pepakriz/phpstan-exception-rules": "~0.11.1",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.54",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.4.1"
  },
  "suggest": {
    "laminas/laminas-config": "^3.1.0, to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-servicemanager": "~3.4.1, to use the navigation factories",
    "mezzio/mezzio-authorization": "to provide ACL-based access restrictions to pages",
    "mezzio/mezzio-router": "^3.0.0, to use router-based URI generation with Mvc pages",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "to use the navigation view helpers"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\Navigation\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\Navigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\Navigation\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-navigation/issues",
    "source": "https://github.com/mimmi20/mezzio-navigation"
  }
}
